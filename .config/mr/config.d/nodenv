[${XDG_CONFIG_HOME:-$HOME/.config}/vcsh/repo.d/nodenv.git]
checkout = vcsh clone https://github.com/marklee77/vcsh-homedir-nodenv.git nodenv
commit   = vcsh nodenv commit -a
update   = vcsh nodenv pull
push     = vcsh nodenv push git@github.com:marklee77/vcsh-homedir-nodenv.git 
status   = vcsh nodenv status
gc       = vcsh nodenv gc

[.nodenv]
checkout = git clone https://github.com/OiNutter/nodenv.git .nodenv
skip     = test "$1" = push

[.nodenv/plugins/node-build]
checkout = git clone https://github.com/OiNutter/node-build.git node-build
skip     = test "$1" = push
